{
  "version": 2,
  "builds": [
    {
      "src": "backend/vercel_app.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.11"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "backend/vercel_app.py"
    }
  ],
  "env": {
    "PYTHONPATH": "backend"
  }
} 